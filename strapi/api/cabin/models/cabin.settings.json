{
  "kind": "collectionType",
  "collectionName": "cabins",
  "info": {
    "name": "cabin",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "owner": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "owned_cabins"
    },
    "bookings": {
      "type": "component",
      "repeatable": true,
      "component": "booking.booking"
    },
    "about": {
      "type": "string"
    },
    "fixed_routines": {
      "type": "component",
      "repeatable": true,
      "component": "fixed-routine.fixed-routines"
    },
    "shopping_list": {
      "type": "component",
      "repeatable": true,
      "component": "shopping-list.shopping-list"
    },
    "not_approved_users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "cabins_awaiting_approval",
      "dominant": true
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "cabins",
      "dominant": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "location": {
      "type": "enumeration",
      "enum": [
        "sea",
        "mountain",
        "lake"
      ],
      "default": "mountain"
    }
  }
}
